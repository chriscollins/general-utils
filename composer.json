{
    "name": "chriscollins/general-utils",
    "description": "General PHP utilities.",
    "keywords": ["Utilities", "Curl", "JSON"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Collins",
            "email": "chrisjamescollins@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "symfony/finder": "~2|~3|~4|~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.23",
        "squizlabs/php_codesniffer": "~3.7.1"
    },
    "autoload": {
        "psr-4": { "ChrisCollins\\GeneralUtils\\": "lib/ChrisCollins/GeneralUtils" }
    },
    "autoload-dev": {
        "psr-4": { "ChrisCollins\\GeneralUtils\\Test\\": "test/ChrisCollins/GeneralUtils/Test" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
